@page "/reservationrequests/create/"
@attribute [Authorize(Roles ="Administrator")]
<h3>Create reservation</h3>

@if (string.IsNullOrEmpty(Message) == false)
{
    <div class="alert alert-danger">
        @Message
    </div>
}

<FormComponent ButtonText="Create" ReservationRequest="reservationRequest" OnValidSubmit="@CreateReservationRequestForm" />